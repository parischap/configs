{
  "extends": [
    "./tsconfig.base.json"
  ],
  "exclude": [
    ".git",
    "dist",
    ".tsbuildinfo",
    "node_modules",
    "packages",
    "esm",
    "examples",
    "tests",
    "readme-assets",
    "vite.config.ts.timestamp-*.mjs"
  ],
  "references": [
    {
      "path": "tsconfig.esm.json"
    }
  ],
  "compilerOptions": {
    "tsBuildInfoFile": ".tsbuildinfo/others.tsbuildinfo",
    "rootDir": ".",
    "outDir": "dist/others",
    "declarationDir": "dist/others/dts",
    "isolatedModules": false,
    "isolatedDeclarations": false,
    "allowJs": true,
    "checkJs": true
  }
}