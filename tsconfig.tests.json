{
  "extends": [
    "./tsconfig.base.json"
  ],
  "include": [
    "tests/**/*.js",
    "tests/**/*.mjs",
    "tests/**/*.cjs",
    "tests/**/*.ts",
    "tests/**/*.mts",
    "tests/**/*.cts"
  ],
  "references": [
    {
      "path": "tsconfig.esm.json"
    }
  ],
  "compilerOptions": {
    "declarationDir": "dist/dts",
    "outDir": "dist",
    "tsBuildInfoFile": ".tsbuildinfo/tests.tsbuildinfo"
  }
}